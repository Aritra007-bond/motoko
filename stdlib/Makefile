$(OUTDIR)/ProduceExchangeLoadQuery_5_1.wasm.csv: $(OUTDIR)/ProduceExchangeLoadQuery_5_1.wasm $(STOPWATCH)
	rm -rf ./.dvm
	$(DVM) reset
	$(DVM) new $(OUTDIR)/ProduceExchangeLoadQuery_5_1.wasm
	$(STOPWATCH) "$(DVM) run 1 start " "5, 1, " > $@
	cat $@

